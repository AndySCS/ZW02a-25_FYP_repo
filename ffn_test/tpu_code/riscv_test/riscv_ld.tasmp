// Load base address of buffer using addi
addi x1, x0, 2
addi x5, x0, 16            // Load FFFFFFFE into x5
lb x6, x5 ,0              // Load byte at offset 0 into x6
lb x7, x1 ,0              // Load byte at offset 1 into x7

// Load base address of halfwords using addi
lh x9, x5 ,0              // Load halfword at offset 0 into x9
lh x10, x1 ,0             // Load halfword at offset 2 into x10

// Load base address of words using addi
lw x12, x5 ,0            // Load word at offset 0 into x12
lw x13, x1 ,0            // Load word at offset 4 into x13

// Load Byte Unsigned (lbu)
lbu x14, x5, 0            // Load unsigned byte at offset 0 into x14
lbu x15, x5, 0            // Load unsigned byte at offset 1 into x15

// Load Halfword Unsigned (lhu)
lhu x16, x5, 0            // Load unsigned halfword at offset 0 into x16
lhu x17, x5, 0            // Load unsigned halfword at offset 2 into x17
add x0, x0, x0

